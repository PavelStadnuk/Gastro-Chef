{
    "semi": true,
    "trailingComma": "none",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "as-needed",
    "parser": "babel",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "auto",
    "bracketSameLine": false,
    "singleAttributePerLine": false,
    "overrides": [
        {
            "files": "*.{js,jsx}",
            "options": {
                "parser": "babel"
            }
        },
        {
            "files": "*.{ts,tsx}",
            "options": {
                "parser": "typescript"
            }
        },
        {
            "files": "*.css",
            "options": {
                "parser": "css"
            }
        },
        {
            "files": "*.scss",
            "options": {
                "parser": "scss"
            }
        }
    ]
}